<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>sql queries</comment>
    <entry key="getUserById">select * from users where id = ?</entry>
    <entry key="getUserId">select * from users where login = ? and password = ?</entry>
    <entry key="insertUser">insert into users (id,login, password) values (?,?,?)</entry>
    <entry key="deleteUser">delete from users where id = ?</entry>
</properties>
